doing:    BaseSymbol([Text("Things I'm doing"), ]),
walking:  Alledge("Walking down the street","doing"),
exercise: BaseSymbol(["Things a human might do","Physical Exercise"]),
cat:      Categorize("walking","exercise"),
ps_s:     BaseSymbol(["Part of Speech", "Sentence"]),
