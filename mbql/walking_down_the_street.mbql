ps_nn:    BaseSymbol(["Part of Speech", "Singular Noun"])),
ps_vp:    BaseSymbol(["Part of Speech", "Verb Phrase"]),
ps_vbd:   BaseSymbol(["Part of Speech", "Verb, Past Tense"]),
said:     BaseSymbol(["Things I said"]),
saidcat:  Categorize("s",":said"),
1s:       Alledge("The dog jumped",[":ps_s"]),
1np:      Alledge("the dog",[":ps_np"]),
1dt:      Alledge("the",[":ps_dt"]),
1nn:      Alledge("dog",[":ps_nn"]),
1vp:      Alledge("jumped",[":ps_vp"]),
1vbd:     Alledge("jumped",[":ps_vbd"]), 