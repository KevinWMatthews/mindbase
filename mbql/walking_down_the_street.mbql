@me =     Agent(default)
$png =  {"IANA Media Type" : "image/png"}
         Alledge( ($png : {"MIME Type" : "image/png"}) : {"Compatible Ontologies"} )
         Alledge( ($png : {".png"}) : {"File Extension"} )
@pic =   DataNode($png; )
@alice =  "Alice"

# Create a unique entity referencing the $alice_text artifact
$my_alice =   Symbolize(@alice)
$alice_persondom = Alledge($alice : GroundConcept("A Human Person"))

# We can symbolize things inline if we want, though they may be unanchored
# This will record the root of a tree of SEVEN Allegations (or four new Symbols, and three allegations)
# S1[Smile]   S2[Mouth]      S3[Wink]    S4[Eye]
#  \__________/               \__________/
#      [A1]                       [A2]
#        \_________________________/
#                    [A3] (Emotive Movement)
$emote = Alledge("Smile" : "Mouth" :: "Wink" : "Eye")


# This will work similarly to the above, except that we are grounding out all of the symbols we are using, rather than declaring wholly new symbols
# This grounding is *necessarily flawed* â€“ There is no perfect symbolic grounding which is possible. Only incremental refinement
$emote2 = Alledge(GroundConcept("Gesture" "Smile") : GroundConcept("Body Part" "Mouth") :: GroundConcept("Gesture" "Wink") : GroundConcept("Body Part" "Eye"))

$pic_of_alice = Alledge($pic : $alice :: : GroundConcept("Photographic Representation") : GroundConcept("A Human Person"))

$doing    =  GroundConcept("Things I'm doing")
$walking  =  Alledge("Walking down the street","doing")
$exercise =  Ground("Things a human might do","Physical Exercise")
$cat      =  Categorize($walking "exercise")
$ps       =  Ground("Human Language" "Part of Speech")
$ps_s     =  Ground($ps "Sentence"])
$ps_np    =  Ground($ps "Noun Phrase"]))
$ps_dt    =  Ground($ps "Determiner"])
$ps_nn    =  Ground($ps "Singular Noun"]))
$ps_vp    =  Ground($ps "Verb Phrase"])
$ps_vbd   =  Ground($ps "Verb, Past Tense"])
$said     =  Ground("Things I said")
$saidcat  =  Alledge($s : $said)
$1s       =  Alledge("The dog jumped" : $ps_s)
$1np      =  Alledge("the dog" $ ps_np)
$1dt      =  Alledge("the" : $ps_dt)
$1nn      =  Alledge("dog" : $ps_nn)
$1vp      =  Alledge("jumped" : $ps_vp)
$1vbd     =  Alledge("jumped" : $ps_vbd)