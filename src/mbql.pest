id = { ASCII_ALPHANUMERIC+ }
string = ${ "\"" ~ inner ~ "\"" }
inner = @{ char* }
char = {
    !("\"" | "\\") ~ ANY
    | "\\" ~ ("\"" | "\\" | "/" | "b" | "f" | "n" | "r" | "t")
    | "\\" ~ ("u" ~ ASCII_HEX_DIGIT{4})
}

line = { SOI ~ id ~ ":" ~ expression ~ EOI }

expression = { default_agent | ground_symbol | alledge }
default_agent =  {"DefaultAgent"}
alledge =  {"Alledge(" ~ symbol ~ ")"}
ground_symbol = { "GroundSymbol" }
symbol = { string }

WHITESPACE = _{ " " | "\t" }
